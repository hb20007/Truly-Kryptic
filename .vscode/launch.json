{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "url": "http://localhost:8080*",
            "webRoot": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run Protractor",
            "program": "${workspaceRoot}/node_modules/.bin/protractor",
            "args": [
                "${workspaceRoot}/protractor.conf.js"
            ]
        }
    ]
}